{
  "name": "personal-library-manager-client",
  "version": "1.0.0",
  "description": "Frontend for Personal Library Manager Application",
  "private": true,
  "proxy": "http://localhost:5000",
  "dependencies": {
    "axios": "^1.3.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.10.0",
    "react-scripts": "5.0.1"
  },
  "devDependencies": {
    "dotenv-cli": "^7.4.4"
  },
  "scripts": {
    "start": "dotenv -e ../.env react-scripts start",
    "build": "dotenv -e ../.env react-scripts build",
    "test": "dotenv -e ../.env react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
